{
  "注释说明": "这是一个示例文件，展示如何在 pipeline 中使用自定义动作",

  "初始化药水数据": {
    "action": {
      "type": "Custom",
      "param": {
        "custom_action": "init_potion_data"
      }
    },
    "next": ["使用AP药水"]
  },

  "使用AP药水": {
    "action": {
      "type": "Custom",
      "param": {
        "custom_action": "use_ap_potion",
        "custom_action_param": "{\"small_ap_limit\": 60, \"big_ap_limit\": 999}"
      }
    },
    "next": ["使用BC药水"]
  },

  "使用BC药水": {
    "action": {
      "type": "Custom",
      "param": {
        "custom_action": "use_bc_potion",
        "custom_action_param": "{\"small_bc_limit\": 60, \"big_bc_limit\": 999}"
      }
    },
    "next": ["显示药水统计"]
  },

  "显示药水统计": {
    "action": {
      "type": "Custom",
      "param": {
        "custom_action": "show_potion_stats"
      }
    }
  }
}
